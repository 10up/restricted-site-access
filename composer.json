{
	"name": "10up/restricted-site-access",
	"description": "WordPress plugin which limits access to visitors who are logged in or allowed by IP addresses.",
	"version": "7.2.0",
	"type": "wordpress-plugin",
	"keywords": [],
	"homepage": "https://10up.com",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "10up",
			"email": "opensource@10up.com",
			"homepage": "https://10up.com",
			"role": "Developer"
		}
	],
	"require-dev": {
		"phpunit/phpunit": "^7.5",
		"dealerdirect/phpcodesniffer-composer-installer": "*",
		"phpcompatibility/php-compatibility": "*",
		"wp-coding-standards/wpcs": "*",
		"10up/wpacceptance": "dev-master"
	},
	"scripts": {
		"test": "phpunit",
		"test:acceptance": "wpacceptance run",
		"lint": "./vendor/bin/phpcs --extensions=php -s .",
		"lint-fix": "./vendor/bin/phpcbf --extensions=php ."
	},
	"minimum-stability": "dev"
}
